        <nav>
          <ul class="nav__menu--pages">
            {%- for item in collections.sub | eleventyNavigation -%}
                {%- set isCurrent = (item.url == page.url) -%}
                {%- set isParent = item.url != '/' and item.url in page.url -%}
                <li class="nav__item{% if isCurrent or isParent %} nav__item--active{% endif %}">
                    <a class="nav__link" href="{{ item.url | url }}"{% if isCurrent %}aria-current="page"{% endif %}>
                        {{ item.title }}
                    </a>
                </li>
